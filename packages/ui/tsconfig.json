{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@my/ui": ["packages/ui/src"]
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
